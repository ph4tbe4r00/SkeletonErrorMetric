function [labeled] = threshholdStuff(labeled)

ThRESH = 300;

unlabeledKeys = unique(labeled);
        for j = 1:size(labeled,3)
        for i = 1:length(unlabeledKeys)

        slice = labeled(:,:,j);
        
        if length(find(slice == unlabeledKeys(i))) < ThRESH
            keyboard
            slice( slice == unlabeledKeys(i)) = -1;
        end            
    end
end

end

